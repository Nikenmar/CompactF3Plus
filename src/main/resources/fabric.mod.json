{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "license": "${mod_license}",
  "icon": "icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.nikenmar.compactf3plus.CompactF3Plus"
    ],
    "modmenu": [
      "net.nikenmar.compactf3plus.CompactF3PlusModMenu"
    ]
  },
  "mixins": [
    "compactf3plus.mixins.json"
  ],
  "depends": {
    "fabricloader": "${loader_version_range}",
    "minecraft": "${minecraft_version_range}",
    "java": ">=21",
    "fabric-api": "*",
    "modmenu": "*",
    "cloth-config": "*"
  }
}
